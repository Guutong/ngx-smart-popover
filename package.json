{
    "name": "ngx-smart-popover-demo",
    "version": "1.4.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "build:all": "ng build && npm run build:lib",
        "build:lib": "ng build ngx-smart-popover && npm run build:lib:scss && npm run build:lib:css",
        "build:lib:css": "node-sass ./projects/ngx-smart-popover/src/lib/popover-content.component.scss -o ./dist/ngx-smart-popover/css/",
        "build:lib:scss": "mkdir ./dist/ngx-smart-popover/scss/ && cp ./projects/ngx-smart-popover/src/lib/popover-content.component.scss ./dist/ngx-smart-popover/scss/popover-content.component.scss",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "e2e:entrypoint": "npm install && ng e2e",
        "e2e:docker": "docker build -t ngx-smart-popover-e2e-tests . && docker run -v ~/code/ngx-smart-popover:/usr/src/app --rm ngx-smart-popover-e2e-tests",
        "run": "ng-packagr -p package.json",
        "publish": "cp README.md dist/ngx-smart-popover/README.md && cd dist/ngx-smart-popover && npm publish"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "9.1.4",
        "@angular/common": "9.1.4",
        "@angular/compiler": "9.1.4",
        "@angular/core": "9.1.4",
        "@angular/forms": "9.1.4",
        "@angular/platform-browser": "9.1.4",
        "@angular/platform-browser-dynamic": "9.1.4",
        "@angular/router": "9.1.4",
        "bootstrap": "4.4.1",
        "core-js": "3.6.5",
        "node-sass": "4.14.1",
        "rxjs": "6.5.5",
        "tslib": "1.10.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.901.4",
        "@angular-devkit/build-ng-packagr": "~0.901.4",
        "@angular/cli": "9.1.4",
        "@angular/compiler-cli": "9.1.4",
        "@angular/language-service": "9.1.4",
        "@types/jasmine": "3.5.10",
        "@types/jasminewd2": "2.0.8",
        "@types/node": "^12.11.1",
        "codelyzer": "^5.1.2",
        "jasmine-core": "3.5.0",
        "jasmine-spec-reporter": "5.0.2",
        "karma": "5.0.4",
        "karma-chrome-launcher": "3.1.0",
        "karma-coverage-istanbul-reporter": "3.0.2",
        "karma-jasmine": "3.1.1",
        "karma-jasmine-html-reporter": "1.5.3",
        "ng-packagr": "^9.0.0",
        "protractor": "5.4.4",
        "protractor-jasmine2-screenshot-reporter": "0.5.0",
        "protractor-screenshot-extension": "1.0.7",
        "scss-bundle": "3.1.1",
        "ts-node": "8.10.1",
        "tslint": "6.1.2",
        "typescript": "3.8.3"
    }
}
